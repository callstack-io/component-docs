{
  "name": "component-docs",
  "version": "0.14.2",
  "description": "Documentation for React components",
  "scripts": {
    "build": "babel -sd dist/ src/ --copy-files && linaria -o dist/css -r src -i dist src/**/*.js",
    "lint:js": "eslint .",
    "lint:css": "stylelint src/**/*.js",
    "lint": "yarn lint:js && yarn lint:css",
    "flow": "flow",
    "prepare": "yarn build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "babel-node example/index",
    "prebuild": "del dist/",
    "preexample": "del example/dist && yarn build",
    "precommit": "yarn flow && yarn lint",
    "release": "release-it"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "files": [
    "dist/",
    "components.js"
  ],
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "chalk": "^2.4.1",
    "css-hot-loader": "^1.4.2",
    "css-loader": "^1.0.0",
    "dashify": "^1.0.0",
    "dedent": "^0.7.0",
    "doctrine": "^2.1.0",
    "escape-html": "^1.0.3",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "history": "^4.7.2",
    "ignore-styles": "^5.0.1",
    "linaria": "^1.0.0-beta.8",
    "markdown-it-github-headings": "^1.1.1",
    "marked": "^0.5.2",
    "mini-css-extract-plugin": "^0.4.0",
    "node-watch": "^0.5.8",
    "opn": "^5.3.0",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.1",
    "react": "~16.5.2",
    "react-docgen": "3.0.0-rc.1",
    "react-dom": "~16.5.2",
    "recast": "^0.15.5",
    "redbox-react": "^1.6.0",
    "sanitize-html": "^1.19.1",
    "webpack": "^4.8.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@callstack/eslint-config": "^2.0.0",
    "babel-eslint": "^9.0.0",
    "conventional-changelog-cli": "^2.0.5",
    "del-cli": "^1.1.0",
    "eslint": "^5.6.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.81.0",
    "husky": "^0.14.3",
    "prettier": "^1.12.1",
    "release-it": "^7.6.1",
    "stylelint": "^9.5.0",
    "stylelint-config-recommended": "^2.1.0"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/satya164/component-docs.git"
  },
  "resolutions": {
    "**/eslint-config-airbnb": "15.1.0",
    "**/eslint-plugin-jsx-a11y": "5.1.1"
  }
}
