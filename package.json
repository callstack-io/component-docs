{
  "name": "component-docs",
  "version": "0.20.3",
  "description": "Documentation for React components",
  "bin": "bin/component-docs.js",
  "main": "dist/index.js",
  "files": [
    "bin/",
    "dist/",
    "components.js"
  ],
  "author": "",
